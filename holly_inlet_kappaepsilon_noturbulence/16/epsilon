FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "16";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 2040;

boundaryField
{
    inletFace
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 2040;
    }
    inletWalls
    {
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        type            epsilonWallFunction;
        value           uniform 2040;
    }
    outletInlet
    {
        type            zeroGradient;
    }
}


// ************************************************************************* //
