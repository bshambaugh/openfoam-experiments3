FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "23";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.648;

boundaryField
{
    inletFace
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.648;
    }
    inletWalls
    {
        type            kqRWallFunction;
        value           uniform 0.648;
    }
    outletInlet
    {
        type            zeroGradient;
    }
}


