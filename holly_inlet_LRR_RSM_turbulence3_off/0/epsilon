FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 2040.0;

boundaryField
{
    inletFace
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 2040.0;
    }
    inletWalls
    {
        type            epsilonWallFunction;
        value           uniform 2040.0;
    }
    outletInlet
    {
        type            cyclicAMI;
    }
    fineSymmetryWall
    {
        type            symmetryPlane;
    }
    fineWalls
    {
        type           epsilonWallFunction;
        value          uniform 2040.0;
    }
    fineCyclicBoundary
    {
        type            zeroGradient;
    }
    fineplug
    {
        type            cyclicAMI;
    }
    faceFine
    {
        type            zeroGradient;
    }
}


